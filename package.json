{
  "name": "bunq.js",
  "version": "0.0.14",
  "main": "lib/Bunq.js",
  "license": "MIT",
  "url": "https://github.com/daanpeer/bunq.js",
  "email": "daan.gdh@gmail.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/daanpeer/bunq.js"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "node-fetch": "^1.6.3",
    "nodemon": "^1.11.0"
  },
  "scripts": {
    "lint": "yarn standard",
    "test": "yarn build && mocha --compilers js:babel-register",
    "test:watch": "yarn test -- --watch",
    "build": "babel src/ -d lib/ && yarn flow",
    "flow": "flow-copy-source -v src lib",
    "prepublishOnly": "yarn lint && yarn test && yarn flow",
    "precommit": "yarn test && yarn lint",
    "watch": "nodemon --watch src --exec yarn build"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "flow-bin": "^0.44.0",
    "flow-copy-source": "^1.1.0",
    "husky": "^0.13.3",
    "mocha": "^3.2.0",
    "nock": "^9.0.13",
    "sinon": "^2.1.0",
    "standard": "^10.0.1"
  },
  "standard": {
    "plugins": [
      "flowtype"
    ],
    "ignore": [
      "/lib",
      "/flow-typed/*",
      "example"
    ],
    "parser": "babel-eslint"
  }
}
